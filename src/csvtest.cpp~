#include <iostream>
#include <fstream
#include <boost/algorithm/string.hpp>

using namespace std;
using namespace boost;

int main(){

  string drone_info_path = "../data/data_preprocessed.csv";
  //open up the file for reading
  ifstream file ("../data/data_preprocessed.csv");
 
  string line;
  
  while ( file.good() ) {
    
    getline ( file, line, '\n' ); 

    vector<string> strs;
    split(strs, line, is_any_of(", "));

    cout << line << '\n'; 

    for (int i = 0, i < strs.size(), i++) {

      cout << strs[i];

    }

    cout << '\n'; 

  }

}
